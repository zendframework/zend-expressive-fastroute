{
    "name": "zendframework/zend-expressive-fastroute",
    "description": "FastRoute integration for Expressive",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "expressive",
        "FastRoute",
        "http",
        "middleware",
        "psr",
        "psr-7"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev",
            "dev-develop": "2.2-dev"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "fig/http-message-util": "^1.1.2",
        "nikic/fast-route": "^1.2",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0.1",
        "zendframework/zend-expressive-router": "^2.0.1",
        "zendframework/zend-stdlib": "^3.1 || 2.*"
    },
    "require-dev": {
        "malukenho/docheader": "^0.1.5",
        "phpunit/phpunit": "^6.0.7 || ^5.7.14",
        "zendframework/zend-coding-standard": "~1.0.0"
    },
    "conflict": {
        "container-interop/container-interop": "<1.2.0"
    },
    "autoload": {
      "psr-4": {
        "Zend\\Expressive\\Router\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "ZendTest\\Expressive\\Router\\": "test/"
      }
    },
    "scripts": {
        "check": [
            "@license-check",
            "@cs-check",
            "@test"
        ],
        "upload-coverage": "coveralls -v",
        "cs-check": "phpcs --colors",
        "cs-fix": "phpcbf --colors",
        "license-check": "docheader check src/ test/",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --coverage-clover clover.xml"
    }
}
